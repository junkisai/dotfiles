[user]
	name = junkisai
	email = jonki.beam@gmail.com
[core]
	ignorecase = false
	quotepath = false
[rebase]
	autosquash = true
[fetch]
    prune = true
[push]
	default = current
[alias]
	st = status
	br = branch
	co = checkout
    sw = switch
    swc = switch -c
	tree = log --graph --oneline
    logo = log --oneline
    fix = commit --fixup
    rebasef = rebase -i --autosquash
    refresh = !git branch --merged | egrep -v '\\*|develop|main' | xargs git branch -d
